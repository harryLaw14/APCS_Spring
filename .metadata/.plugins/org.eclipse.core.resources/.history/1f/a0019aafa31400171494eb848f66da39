package textExcel;

public class SpreadsheetLocation implements Location{
	private int row;
	private int column;
	
	@Override
  	public int getRow(){
	  	return row;
  	}

	@Override
	public int getCol(){
		return column;
	}
  
	public SpreadsheetLocation(String cellName){
		column = (int)(cellName.charAt(0))-65;
		row = Integer.parseInt(cellName.substring(1))-1;
	}

}
